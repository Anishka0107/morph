.col-xs-12.col-md-9
  .alert.alert-info
    - if @type == "users"
      = no_search_results_message("users", @q)
    - else
      - if !@all_scrapers.total_count.zero? && @scrapers.total_count.zero?
        = no_search_results_message("scrapers <em>with data,</em> that are".html_safe, @q)
      - else
        = no_search_results_message("scrapers", @q)
